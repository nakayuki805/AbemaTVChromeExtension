{
  "name": "abematv-chrome-extension",
  "private": true,
  "version": "0.14.5",
  "description": "AbemaTVを快適にする非公式の拡張機能",
  "scripts": {
    "start": "npm run dev:chrome",
    "build": "npm run build:chrome",
    "build:chrome": "gulp pack --production --vendor=chrome",
    "build:firefox": "gulp pack --production --vendor=firefox",
    "build:opera": "gulp pack --production --vendor=opera",
    "build:edge": "gulp pack --production --vendor=edge",
    "dev": "npm run dev:chrome",
    "dev:chrome": "gulp --watch --vendor=chrome",
    "dev:firefox": "gulp --watch --vendor=firefox",
    "dev:opera": "gulp --watch --vendor=opera",
    "dev:edge": "gulp --watch --vendor=edge",
    "lint": "standard",
    "version:patch": "gulp patch",
    "version:feature": "gulp feature",
    "version:release": "gulp release"
  },
  "standard": {
    "globals": [
      "chrome"
    ]
  },
  "devDependencies": {
    "@types/chrome": "0.0.61",
    "@types/jquery": "^3.3.4",
    "@types/node": "^9.6.23",
    "babel-cli": "6.x.x",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "chai": "4.x.x",
    "chromereload": "0.x.x",
    "debounce": "1.x.x",
    "del": "3.x.x",
    "gulp": "^4.0.0",
    "gulp-bump": "^3.1.1",
    "gulp-cache": "1.x.x",
    "gulp-clean-css": "^3.9.4",
    "gulp-filter": "^5.x.x",
    "gulp-git": "^2.7.0",
    "gulp-hub": "^0.8.0",
    "gulp-if": "2.x.x",
    "gulp-imagemin": "4.x.x",
    "gulp-json-transform": "0.x.x",
    "gulp-less": "4.x.x",
    "gulp-livereload": "3.x.x",
    "gulp-plumber": "1.x.x",
    "gulp-sass": "^4.0.1",
    "gulp-sequence": "1.x.x",
    "gulp-sourcemaps": "^2.x.x",
    "gulp-tag-version": "1.x.x",
    "gulp-util": "3.x.x",
    "gulp-zip": "^4.x.x",
    "require-dir": "1.x.x",
    "standard": "^11.0.1",
    "ts-loader": "^4.4.2",
    "tslint": "^5.10.0",
    "vinyl-named": "1.x.x",
    "webpack": "^4.15.1",
    "webpack-stream": "^4.0.3",
    "yargs": "^11.1.0"
  },
  "dependencies": {
    "cw-log": "^0.2.0",
    "jquery": "^3.2.1",
    "typescript": "^2.9.2"
  }
}
